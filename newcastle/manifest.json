{
  "name": "Share to Newcastle",
  "short_name": "Newcastle",
  "start_url": "./",
  "scope": "./",
  "display": "standalone",
  "icons": [
    { "src": "../small_icon.png", "sizes": "192x192", "type": "image/png" },
    { "src": "../large_icon.png", "sizes": "512x512", "type": "image/png" }
  ],
  "share_target": {
    "action": "./upload", 
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": { "files": [{ "name": "photos", "accept": ["image/*"] }] }
  }
}